{
  "name": "@allaround/frontend",
  "version": "0.0.0",
  "description": "AllAround frontend",
  "scripts": {
    "start:prod": "STAGING_ENV=prod webpack serve --mode=production --host 0.0.0.0",
    "start:dev": "STAGING_ENV=dev webpack serve --mode=development --host 0.0.0.0",
    "build:prod": "STAGING_ENV=prod webpack --config webpack.config.js --mode production",
    "cy:run": "cypress run --e2e --browser chrome",
    "cy:open": "cypress open --e2e --browser chrome",
    "tsc:config": "tsc --showConfig"
  },
